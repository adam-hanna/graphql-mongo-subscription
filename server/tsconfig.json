{
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  },
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "esnext",
      "esnext.asynciterable"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "types": [
      "node"
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./@types"
    ],
    "baseUrl": ".",
    "paths": {
      "@data": ["src/data"],
      "@schema": ["src/data/schema"]
    },
    "downlevelIteration": true
  },
  "include": [
    "src"
  ]
}
